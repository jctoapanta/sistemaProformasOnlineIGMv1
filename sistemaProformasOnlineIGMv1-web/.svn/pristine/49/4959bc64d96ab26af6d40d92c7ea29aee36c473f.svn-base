<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="TitemEditDlg" widgetVar="TitemEditDialog" modal="true" resizable="false" appendTo="@(body)" header="#{bundle.EditTitemTitle}">
            <h:form id="TitemEditForm">
                <h:panelGroup id="display">
                    <p:panelGrid columns="2" rendered="#{titemController.selected != null}">
                        <p:outputLabel value="#{bundle.EditTitemLabel_idItem}" for="idItem" />
                        <p:inputText id="idItem" value="#{titemController.selected.idItem}" title="#{bundle.EditTitemTitle_idItem}" required="true" requiredMessage="#{bundle.EditTitemRequiredMessage_idItem}"/>
                        <p:outputLabel value="#{bundle.EditTitemLabel_descItem}" for="descItem" />
                        <p:inputText id="descItem" value="#{titemController.selected.descItem}" title="#{bundle.EditTitemTitle_descItem}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_pvp}" for="pvp" />
                        <p:inputText id="pvp" value="#{titemController.selected.pvp}" title="#{bundle.EditTitemTitle_pvp}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_costo}" for="costo" />
                        <p:inputText id="costo" value="#{titemController.selected.costo}" title="#{bundle.EditTitemTitle_costo}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_LIva}" for="LIva" />
                        <p:inputText id="LIva" value="#{titemController.selected.LIva}" title="#{bundle.EditTitemTitle_LIva}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_estado}" for="estado" />
                        <p:inputText id="estado" value="#{titemController.selected.estado}" title="#{bundle.EditTitemTitle_estado}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_LInventariado}" for="LInventariado" />
                        <p:inputText id="LInventariado" value="#{titemController.selected.LInventariado}" title="#{bundle.EditTitemTitle_LInventariado}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_idCreador}" for="idCreador" />
                        <p:inputText id="idCreador" value="#{titemController.selected.idCreador}" title="#{bundle.EditTitemTitle_idCreador}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_idModificador}" for="idModificador" />
                        <p:inputText id="idModificador" value="#{titemController.selected.idModificador}" title="#{bundle.EditTitemTitle_idModificador}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_fechaCreacion}" for="fechaCreacion" />
                        <p:calendar id="fechaCreacion" pattern="MM/dd/yyyy HH:mm:ss" value="#{titemController.selected.fechaCreacion}" title="#{bundle.EditTitemTitle_fechaCreacion}"  showOn="button"/>
                        <p:outputLabel value="#{bundle.EditTitemLabel_descripcionAnterior}" for="descripcionAnterior" />
                        <p:inputText id="descripcionAnterior" value="#{titemController.selected.descripcionAnterior}" title="#{bundle.EditTitemTitle_descripcionAnterior}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_LVentaOnline}" for="LVentaOnline" />
                        <p:inputText id="LVentaOnline" value="#{titemController.selected.LVentaOnline}" title="#{bundle.EditTitemTitle_LVentaOnline}" />
                        <p:outputLabel value="#{bundle.EditTitemLabel_idFamilia}" for="idFamilia" />
                        <p:selectOneMenu id="idFamilia" value="#{titemController.selected.idFamilia}" required="true" requiredMessage="#{bundle.EditTitemRequiredMessage_idFamilia}">
                            <f:selectItem itemLabel="#{bundle.SelectOneMessage}"/>
                            <f:selectItems value="#{tfamiliaController.itemsAvailableSelectOne}"
                                           var="idFamiliaItem"
                                           itemValue="#{idFamiliaItem}"/>
                        </p:selectOneMenu>
                        <p:outputLabel value="#{bundle.EditTitemLabel_idUnidad}" for="idUnidad" />
                        <p:selectOneMenu id="idUnidad" value="#{titemController.selected.idUnidad}" >
                            <f:selectItem itemLabel="#{bundle.SelectOneMessage}"/>
                            <f:selectItems value="#{tunidadmedidaController.itemsAvailableSelectOne}"
                                           var="idUnidadItem"
                                           itemValue="#{idUnidadItem}"/>
                        </p:selectOneMenu>
                    </p:panelGrid>
                    <p:commandButton actionListener="#{titemController.update}" value="#{bundle.Save}" update="display,:TitemListForm:datalist,:growl" oncomplete="handleSubmit(args, 'TitemEditDialog');"/>
                    <p:commandButton value="#{bundle.Cancel}" onclick="TitemEditDialog.hide()"/>
                </h:panelGroup>
            </h:form>
        </p:dialog>

    </ui:composition>
</html>
