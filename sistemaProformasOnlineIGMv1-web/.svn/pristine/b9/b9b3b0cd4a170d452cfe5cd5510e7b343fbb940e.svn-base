<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="TplotersCreateDlg" widgetVar="TplotersCreateDialog" modal="true" resizable="false" appendTo="@(body)" header="#{bundle.CreateTplotersTitle}">
            <h:form id="TplotersCreateForm">
                <h:panelGroup id="display">
                    <p:panelGrid columns="2" rendered="#{tplotersController.selected != null}">
                        <p:outputLabel value="#{bundle.CreateTplotersLabel_idPloter}" for="idPloter" />
                        <p:inputText id="idPloter" value="#{tplotersController.selected.idPloter}" title="#{bundle.CreateTplotersTitle_idPloter}" required="true" requiredMessage="#{bundle.CreateTplotersRequiredMessage_idPloter}"/>
                        <p:outputLabel value="#{bundle.CreateTplotersLabel_descPloter}" for="descPloter" />
                        <p:inputText id="descPloter" value="#{tplotersController.selected.descPloter}" title="#{bundle.CreateTplotersTitle_descPloter}" required="true" requiredMessage="#{bundle.CreateTplotersRequiredMessage_descPloter}"/>
                        <p:outputLabel value="#{bundle.CreateTplotersLabel_control}" for="control" />
                        <p:inputText id="control" value="#{tplotersController.selected.control}" title="#{bundle.CreateTplotersTitle_control}" />
                    </p:panelGrid>
                    <p:commandButton actionListener="#{tplotersController.create}" value="#{bundle.Save}" update="display,:TplotersListForm:datalist,:growl" oncomplete="handleSubmit(args,'TplotersCreateDialog');"/>
                    <p:commandButton value="#{bundle.Cancel}" onclick="TplotersCreateDialog.hide()"/>
                </h:panelGroup>
            </h:form>
        </p:dialog>

    </ui:composition>
</html>
