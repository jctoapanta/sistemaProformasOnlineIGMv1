<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition>

        <p:dialog id="TtipocartaViewDlg" widgetVar="TtipocartaViewDialog" modal="true" resizable="false" appendTo="@(body)" header="#{bundle.ViewTtipocartaTitle}">
            <h:form id="TtipocartaViewForm">
                <h:panelGroup id="display">
                    <p:panelGrid columns="2" rendered="#{ttipocartaController.selected != null}">
                        <h:outputText value="#{bundle.ViewTtipocartaLabel_idTipoCarta}"/>
                        <h:outputText value="#{ttipocartaController.selected.idTipoCarta}" title="#{bundle.ViewTtipocartaTitle_idTipoCarta}"/>
                        <h:outputText value="#{bundle.ViewTtipocartaLabel_descCarta}"/>
                        <h:outputText value="#{ttipocartaController.selected.descCarta}" title="#{bundle.ViewTtipocartaTitle_descCarta}"/>
                        <h:outputText value="#{bundle.ViewTtipocartaLabel_tamanio}"/>
                        <h:outputText value="#{ttipocartaController.selected.tamanio}" title="#{bundle.ViewTtipocartaTitle_tamanio}"/>
                        <h:outputText value="#{bundle.ViewTtipocartaLabel_tipoTamanio}"/>
                        <h:outputText value="#{ttipocartaController.selected.tipoTamanio}" title="#{bundle.ViewTtipocartaTitle_tipoTamanio}"/>
                        <h:outputText value="#{bundle.ViewTtipocartaLabel_control}"/>
                        <h:outputText value="#{ttipocartaController.selected.control}" title="#{bundle.ViewTtipocartaTitle_control}"/>
                    </p:panelGrid>
                    <p:commandButton value="#{bundle.Close}" onclick="TtipocartaViewDialog.hide()"/>
                </h:panelGroup>
            </h:form>
        </p:dialog>

    </ui:composition>
</html>
