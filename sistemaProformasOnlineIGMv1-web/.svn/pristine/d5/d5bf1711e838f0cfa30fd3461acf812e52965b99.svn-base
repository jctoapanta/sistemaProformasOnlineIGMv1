<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition template="/template.xhtml">


        <ui:define name="title">
            <h:outputText value="#{bundle.ListTdetproformaTitle}"></h:outputText>
        </ui:define>

        <ui:define name="body">
            <h:form id="TdetproformaListForm">
                <p:panel header="#{bundle.ListTdetproformaTitle}">
                    <p:dataTable id="datalist" value="#{tdetproformaController.items}" var="item"
                                 selectionMode="single" selection="#{tdetproformaController.selected}"
                                 paginator="true"
                                 rowKey="#{item.tdetproformaPK}"
                                 rows="10"
                                 rowsPerPageTemplate="10,20,30,40,50"
                                 >

                        <p:ajax event="rowSelect"   update="createButton viewButton editButton deleteButton"/>
                        <p:ajax event="rowUnselect" update="createButton viewButton editButton deleteButton"/>

                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_tdetproformaPK_noReg}"/>
                            </f:facet>
                            <h:outputText value="#{item.tdetproformaPK.noReg}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_detalleItem}"/>
                            </f:facet>
                            <h:outputText value="#{item.detalleItem}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_cantidad}"/>
                            </f:facet>
                            <h:outputText value="#{item.cantidad}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_pvpTotal}"/>
                            </f:facet>
                            <h:outputText value="#{item.pvpTotal}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_formato}"/>
                            </f:facet>
                            <h:outputText value="#{item.formato}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_tipoItem}"/>
                            </f:facet>
                            <h:outputText value="#{item.tipoItem}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_ivaPorcentaje}"/>
                            </f:facet>
                            <h:outputText value="#{item.ivaPorcentaje}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_descuento}"/>
                            </f:facet>
                            <h:outputText value="#{item.descuento}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_LAcepta}"/>
                            </f:facet>
                            <h:outputText value="#{item.LAcepta}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_idFactura}"/>
                            </f:facet>
                            <h:outputText value="#{item.idFactura}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_periodoFactura}"/>
                            </f:facet>
                            <h:outputText value="#{item.periodoFactura}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_periodocodigo}"/>
                            </f:facet>
                            <h:outputText value="#{item.periodocodigo}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_codsec}"/>
                            </f:facet>
                            <h:outputText value="#{item.codsec}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_idCentroGestion}"/>
                            </f:facet>
                            <h:outputText value="#{item.idCentroGestion}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_idProyecto}"/>
                            </f:facet>
                            <h:outputText value="#{item.idProyecto}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_idFacturaAnterior}"/>
                            </f:facet>
                            <h:outputText value="#{item.idFacturaAnterior}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_periodoFacturaAnterior}"/>
                            </f:facet>
                            <h:outputText value="#{item.periodoFacturaAnterior}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_numDesde}"/>
                            </f:facet>
                            <h:outputText value="#{item.numDesde}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_numHasta}"/>
                            </f:facet>
                            <h:outputText value="#{item.numHasta}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_numActual}"/>
                            </f:facet>
                            <h:outputText value="#{item.numActual}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_obsLiberaProf}"/>
                            </f:facet>
                            <h:outputText value="#{item.obsLiberaProf}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_impreso}"/>
                            </f:facet>
                            <h:outputText value="#{item.impreso}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_dimension}"/>
                            </f:facet>
                            <h:outputText value="#{item.dimension}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_fecimp}"/>
                            </f:facet>
                            <h:outputText value="#{item.fecimp}">
                                <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                            </h:outputText>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_idItem}"/>
                            </f:facet>
                            <h:outputText value="#{item.idItem}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_idIva}"/>
                            </f:facet>
                            <h:outputText value="#{item.idIva}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_idPloter}"/>
                            </f:facet>
                            <h:outputText value="#{item.idPloter}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_tproforma}"/>
                            </f:facet>
                            <h:outputText value="#{item.tproforma}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListTdetproformaTitle_idTipoCarta}"/>
                            </f:facet>
                            <h:outputText value="#{item.idTipoCarta}"/>
                        </p:column>
                        <f:facet name="footer">
                            <p:commandButton id="createButton" icon="ui-icon-plus"   value="#{bundle.Create}" actionListener="#{tdetproformaController.prepareCreate}" update=":TdetproformaCreateForm" oncomplete="PF('TdetproformaCreateDialog').show()"/>
                            <p:commandButton id="viewButton"   icon="ui-icon-search" value="#{bundle.View}" update=":TdetproformaViewForm" oncomplete="PF('TdetproformaViewDialog').show()" disabled="#{empty tdetproformaController.selected}"/>
                            <p:commandButton id="editButton"   icon="ui-icon-pencil" value="#{bundle.Edit}" update=":TdetproformaEditForm" oncomplete="PF('TdetproformaEditDialog').show()" disabled="#{empty tdetproformaController.selected}"/>
                            <p:commandButton id="deleteButton" icon="ui-icon-trash"  value="#{bundle.Delete}" actionListener="#{tdetproformaController.destroy}" update=":growl,datalist" disabled="#{empty tdetproformaController.selected}"/>
                        </f:facet>
                    </p:dataTable>
                </p:panel>
            </h:form>

            <ui:include src="Create.xhtml"/>
            <ui:include src="Edit.xhtml"/>
            <ui:include src="View.xhtml"/>
        </ui:define>
    </ui:composition>

</html>
