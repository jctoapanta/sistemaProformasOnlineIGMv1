	<queryString>
		<![CDATA[SELECT "TENTIDAD"."CIU",
    "TENTIDAD"."NOMBRES",
    "TENTIDAD"."APELLIDOS",
    "TDETPROFORMA"."ID_PROFORMA",
    "TENTIDAD"."DIRECCION",
    "TENTIDAD"."TELEFONO_CONVENCIONAL",
